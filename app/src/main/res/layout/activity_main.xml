        android:layout_height="wrap_content"
        android:text="siginOut" />

    <Button
        android:id="@+id/siginFace"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="siginFace" />

    <TextView
        android:id="@+id/status"
